{
    "name": "lingxi/hashids",
    "description": "A Hashids bridge for Laravel",
    "license": "MIT",
    "keywords": ["laravel", "hashids", "hashid", "encrypt", "decrypt"],
    "autoload": {
        "psr-4": {
            "Lingxi\\Hashids\\": "src/"
        },
        "files": [
            "include/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Lingxi\\Tests\\Hashids\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "illuminate/support": "5.5.*|5.6.*",
        "graham-campbell/manager": "^4.0",
        "hashids/hashids": "^2.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^5.0",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5|^7.0"
    }
}
