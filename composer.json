{
    "name": "lingxi/hashids",
    "description": "A Hashids bridge for Laravel",
    "license": "MIT",
    "keywords": ["laravel", "hashids", "hashid", "encrypt", "decrypt"],
    "require": {
        "php": ">=5.6",
        "illuminate/support": "~5.0",
        "graham-campbell/manager": "^2.4",
        "hashids/hashids": "^1.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.2",
        "mockery/mockery": "^0.9.5",
        "phpunit/phpunit": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "Lingxi\\Hashids\\": "src/"
        },
        "files": [
            "include/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Lingxi\\Tests\\Hashids\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
